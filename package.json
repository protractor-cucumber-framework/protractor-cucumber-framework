{
  "name": "protractor-cucumber-framework",
  "version": "4.0.6",
  "description": "Protractor framework for Cucumber.js",
  "main": "index.js",
  "engines": {
    "node": ">=6.9.x"
  },
  "scripts": {
    "webdriver": "webdriver-manager update --standalone --versions.standalone=2.53.1 && webdriver-manager start --versions.standalone=2.53.1 --logging ../../../../logging.properties",
    "start": "node test/testapp/server",
    "lint": "eslint . --fix",
    "pretest": "npm run lint && multidep test/multidep.js",
    "test": "mocha -c -s 10000 -t 20000 test/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/protractor-cucumber-framework/protractor-cucumber-framework"
  },
  "keywords": [
    "angular",
    "test",
    "testing",
    "webdriver",
    "webdriverjs",
    "selenium",
    "protractor",
    "protractor-framework",
    "cucumber",
    "cucumber-js",
    "gherkin",
    "bdd"
  ],
  "author": "Matt Fritz <mfritzer@gmail.com>",
  "contributors": [
    "Matt Fritz <mfritzer@gmail.com>",
    "Darrin Holst <darrinholst@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/protractor-cucumber-framework/protractor-cucumber-framework/issues"
  },
  "homepage": "https://github.com/protractor-cucumber-framework/protractor-cucumber-framework",
  "dependencies": {
    "debug": "^3.0.1",
    "glob": "^7.0.3",
    "q": "^1.4.1",
    "relative": "^3.0.2",
    "tmp": "^0.0.33"
  },
  "peerDependencies": {
    "cucumber": ">= 1.3.0 || >= 2.0.0 || >= 3.0.0",
    "protractor": ">= 3.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-like": "^0.2.14",
    "eslint": "^4.6.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-prettier": "^2.2.0",
    "express": "^4.15.4",
    "httpster": "1.0.3",
    "mocha": "^3.5.0",
    "multidep": "2.0.2",
    "prettier": "^1.6.1",
    "protractor": "^5.1.2"
  },
  "cucumberConf": {
    "version1": "1.3.3",
    "version2": "2.3.1",
    "version3": "3.0.1"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "bracketSpacing": false
  }
}
